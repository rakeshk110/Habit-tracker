{
    "builds": [
        {
            "src": "habit_tracker/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.13.4" }
        }
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "habit_tracker/wsgi.py" 
        }
    ]
}